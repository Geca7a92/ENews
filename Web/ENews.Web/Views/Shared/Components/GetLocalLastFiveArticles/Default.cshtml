@model ENews.Web.ViewModels.Home.IndexViewModel

<div class="card">
    <div class="card-header">
        <h3>Some news</h3>
    </div>
    <div class="card-body">
        @foreach (var article in Model.LatestFiveArticles)
        {
            @await Html.PartialAsync("_ArticlePartial", article)
        }
    </div>
</div>